{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "no-param-reassign": "off",
      "no-undef": "off",
      "no-unused-vars": "off",
      "space-unary-ops": "off"
    }
}
